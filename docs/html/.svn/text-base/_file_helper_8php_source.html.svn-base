<a name="l00312"></a>00312             <span class="comment">//find files/dirs in the dir</span>
<a name="l00313"></a>00313             $srcs = glob(rtrim($dirname,<span class="charliteral">&#39;/&#39;</span>).<span class="stringliteral">&#39;/*&#39;</span>);
<a name="l00314"></a>00314             $files = array();
<a name="l00315"></a>00315             <span class="comment">//get deeper level files</span>
<a name="l00316"></a>00316             <span class="keywordflow">if</span> (!empty($srcs)) {
<a name="l00317"></a>00317                 <span class="keywordflow">foreach</span>($srcs as $src) {
